# URL Koneksi Database PostgreSQL Anda
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"

# Kunci rahasia untuk menandatangani token JWT (JSON Web Token)
JWT_SECRET="kunci_rahasia_anda_yang_sangat_aman"

# Port tempat server akan berjalan
PORT=3000

# (Opsional) Salt rounds untuk hashing password bcrypt
SALT_ROUNDS=10